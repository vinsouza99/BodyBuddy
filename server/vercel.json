{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/learn",
      "headers": {
        "cache-control": "public, max-age=3600, s-maxage=86400"
      }
    }
  ]
}
